#!/Users/alberthan/VSCodeProjects/hdlogger/bin/python3
# EASY-INSTALL-ENTRY-SCRIPT: 'MonkeyType','console_scripts','monkeytype'
__requires__ = 'MonkeyType'
import re
import sys
from pkg_resources import load_entry_point

if __name__ == '__main__':
    sys.argv[0] = re.sub(r'(-script\.pyw?|\.exe)?$', '', sys.argv[0])
    sys.exit(
        load_entry_point('MonkeyType', 'console_scripts', 'monkeytype')()
    )
