#!/Users/alberthan/VSCodeProjects/hdlogger/bin/python3
# EASY-INSTALL-ENTRY-SCRIPT: 'pyinstrument','console_scripts','pyinstrument'
__requires__ = 'pyinstrument'
import re
import sys
from pkg_resources import load_entry_point

if __name__ == '__main__':
    sys.argv[0] = re.sub(r'(-script\.pyw?|\.exe)?$', '', sys.argv[0])
    sys.exit(
        load_entry_point('pyinstrument', 'console_scripts', 'pyinstrument')()
    )
